{
  "name": "@freshworks-jaya/freshchat-api",
  "version": "0.7.20",
  "description": "Provides simple interface for accessing Freshchat's public APIs",
  "repository": "git@github.com:freshdesk/jaya-lib.git",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf lib coverage .nyc_output",
    "watch": "tsc --watch --incremental",
    "coverage": "codecov",
    "test": "nyc --reporter=lcov mocha",
    "build": "tsc && cpx \"src/*.hbs\" ./lib",
    "format": "prettier --write \"src/**/*.ts\"",
    "build:check": "tsc --noEmit",
    "lint": "tsc --noEmit && eslint 'src/**/*.ts' --quiet --fix",
    "prepare": "npm run build",
    "preversion": "npm run lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.16",
    "@types/chai-as-promised": "^7.1.3",
    "@types/handlebars-helpers": "^0.5.2",
    "@types/html-to-text": "^6.0.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.8.1",
    "cpx": "^1.5.0",
    "mocha": "^8.3.2",
    "nock": "^13.0.11",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@freshworks-jaya/utilities": "^0.2.5",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "handlebars": "^4.7.7",
    "handlebars-helpers": "^0.10.0",
    "html-to-text": "7.0.0"
  }
}
